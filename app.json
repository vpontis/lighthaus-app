{
  "expo": {
    "name": "Lighthaus",
    "slug": "lighthausapp",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./assets/lighthouse.png",
    "splash": {
      "image": "./assets/lighthouse.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.opolislabs.lighthaus",
      "supportsTablet": true
    },
    "android": {
      "package": "com.opolislabs.lighthaus"
    }
  }
}
